Header add Access-Control-Allow-Origin "*"
AddCharset utf-8 .html
Options +FollowSymLinks
RewriteEngine On
RewriteCond %{HTTPS} on
RewriteCond %{HTTP_HOST} ^administradora-vickyann.com.ve
RewriteRule (.*) http://www.administradora-vickyann.com.ve/$1 [R=301,L]

Rewriterule ^([A-Za-z\-]+).html$ index.php?accion=$1
#Rewriterule inmobiliaria.html$ inmobiliaria/index.php?accion=inmobiliaria
#Rewriterule ^inmobiliaria/([A-Za-z\-]+)$ inmobiliaria/index.php?accion=$1
#Rewriterule ^inmobiliaria/([A-Za-z\-]+)/([0-9A-Za-z\-\_\.]+)$ inmobiliaria/index.php?accion=$1&id=$2
Rewriterule recuperar-password.html$ index.php?accion=recuperar-password
Rewriterule actualizacionWeb.php$ actualizacionWeb.php
Rewriterule condominio-en-linea$ login.php
Rewriterule ^enlinea/([A-Za-z\-]+)/([A-Za-z\-]+)$ enlinea/$1/index.php?accion=$2 [L]
#Rewriterule ^enlinea/([A-Za-z\-]+)/([A-Za-z\-]+)/([0-9]{1,5})$ enlinea/$1/index.php?accion=$2&id=$3 [L]
Rewriterule ^enlinea/([A-Za-z\-]+)/([A-Za-z\-]+)/([0-9A-Za-z\-\_\.]+)$ enlinea/$1/index.php?accion=$2&id=$3 [L]
Rewriterule ^enlinea/([A-Za-z\-]+)/([A-Za-z\-]+)/pagina/([0-9]+)$ enlinea/$1/index.php?accion=$2&pagina=$3 [L]
Rewriterule ^intranet/([A-Za-z\-]+)/([A-Za-z\-]+)$ intranet/$1/index.php?accion=$2 [L]
Rewriterule ^intranet/([A-Za-z\-]+)/([A-Za-z\-]+)/([0-9A-Za-z\-\_\.]+)$ intranet/$1/index.php?accion=$2&id=$3 [L]
Rewriterule ^imprimirCancelacionDeGastos/([A-Za-z-_0-9]+)$ includes/html2pdf/examples/cancelacion.php?id=$1 [L]

<IfModule mod_expires.c>
ExpiresActive On
ExpiresDefault A0
Header set Connection keep-alive 
<FilesMatch "\.(jpe?g|gif|png|css|js)$">
#tiempo en cachear los archivos
ExpiresDefault A14515200
Header append Cache-Control "public"
</FilesMatch>
</IfModule>

<IfModule mod_headers.c>
Header set Connection keep-alive 
<FilesMatch "\.(jpe?g|gif|png|css|js)$">
#tiempo en cachear los archivos
Header set Cache-Control "max-age=14515200, public"
</FilesMatch>
<FilesMatch "\.(css|js)$">
Header set Vary Accept-Encoding
</FilesMatch>
</IfModule>

<ifModule mod_gzip.c>
mod_gzip_on Yes
mod_gzip_dechunk Yes
mod_gzip_item_include file .(html?|txt|css|js|php|pl)$
mod_gzip_item_include handler ^cgi-script$
mod_gzip_item_include mime ^text/.*
mod_gzip_item_include mime ^application/x-javascript.*
mod_gzip_item_exclude mime ^image/.*
mod_gzip_item_exclude rspheader ^Content-Encoding:.*gzip.*
</ifModule>

<IfModule mod_deflate.c>
    #The following line is enough for .js and .css
    AddOutputFilter DEFLATE js css

    #The following line also enables compression by file content type, for the following list of Content-Type:s
    AddOutputFilterByType DEFLATE text/html text/plain text/xml application/xml text/css text/js

    #The following lines are to avoid bugs with some browsers
    BrowserMatch ^Mozilla/4 gzip-only-text/html
    BrowserMatch ^Mozilla/4\.0[678] no-gzip
    BrowserMatch \bMSIE !no-gzip !gzip-only-text/html 
</IfModule>